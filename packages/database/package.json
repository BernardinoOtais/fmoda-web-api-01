{
  "name": "@repo/db",
  "version": "0.0.0",
  "exports": {
    "./auth": "./dist/prisma-servicos/auth/auth.js",
    "./user": "./dist/user/user.js",
    "./administrador_user": "./dist/administrador/user/index.js",
    "./embarques": "./dist/embarques/raiz/index.js",
    "./embarques_idenvio": "./dist/embarques/id-envio/index.js",
    "./embarques_configurar": "./dist/embarques/configurar/index.js",
    "./qualidade_balancom": "./dist/qualidade/balancom/index.js",
    "./qualidade_balancom_op": "./dist/qualidade/balancom-op/index.js",
    "./qualidade_balancom_op_composicao": "./dist/qualidade/balanco-op-composicao/index.js",
    "./android/auth": "./dist/android/auth/index.js",
    "./android/marrocos/envios": "./dist/android/marrocos/envios/index.js",
    "./android/marrocos/envios/fim": "./dist/android/marrocos/envios/fim/index.js",
    "./android/marrocos/paletes": "./dist/android/marrocos/paletes/index.js",
    "./android/marrocos/caixas": "./dist/android/marrocos/caixas/index.js",
    "./android/especial": "./dist/android/especial/index.js",
    "./android/resumo": "./dist/android/resumo/index.js",
    "./android/cortes/envios": "./dist/android/cortes/envios/index.js",
    "./android/cortes/envios/lotes": "./dist/android/cortes/envios/lotes/index.js",
    "./android/cortes/fim": "./dist/android/cortes/fim/index.js",
    "./planeamento": "./dist/planeamento/index.js",
    "./joana/emmcma": "./dist/joana/entradas-mc-ma/index.js",
    "./joana/esteborda": "./dist/joana/est-e-borda/index.js",
    "./joana/corteporop": "./dist/joana/cortes-por-op/index.js",
    "./joana/faturas": "./dist/joana/faturas/index.js",
    "./joana/faturasplan": "./dist/joana/faturas-planeadas/index.js",
    "./joana/enviosmarrocos": "./dist/joana/envios-marrocos/index.js",
    "./rfid": "./dist/rfid/index.js"
  },
  "scripts": {
    "db:generate": "npm run db:generate:auth && npm run db:generate:envios && npm run db:generate:qualidade && npm run db:generate:androidMarrocos && npm run db:generate:androidCortes",
    "db:generate:auth": "prisma generate --schema=prisma/schema.auth.prisma",
    "db:generate:envios": "prisma generate --schema=prisma/schema.envios.prisma",
    "db:generate:qualidade": "prisma generate --schema=prisma/schema.qualidade.prisma",
    "db:generate:androidMarrocos": "prisma generate --schema=prisma/android/schema.androidMarrocos.prisma",
    "db:generate:androidCortes": "prisma generate --schema=prisma/android/schema.androidCortes.prisma",
    "db:pull": "npm run db:pull:auth && npm run db:pull:envios && npm run db:pull:qualidade && npm run db:pull:androidMarrocos",
    "db:pull:auth": "prisma db pull --schema=prisma/schema.auth.prisma",
    "db:pull:envios": "prisma db pull --schema=prisma/schema.envios.prisma",
    "db:pull:qualidade": "prisma db pull --schema=prisma/schema.qualidade.prisma",
    "db:pull:androidMarrocos": "prisma db pull --schema=prisma/android/schema.androidMarrocos.prisma",
    "db:pull:androidCortes": "prisma db pull --schema=prisma/android/schema.androidCortes.prisma",
    "db:formatAuth": "prisma format --schema=prisma/schema.auth.prisma",
    "db:validateAuth": "prisma validate --schema=prisma/schema.auth.prisma",
    "dev": "concurrently \"tsc --watch\" \"tsc-alias --watch\"",
    "build": "npm run db:generate && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "postbuild": "cp -r src/generated dist/",
    "lint": "eslint . --max-warnings 0"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "prisma": "^6.16.2",
    "tsc-alias": "^1.8.16"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@repo/encryption": "*",
    "@repo/tipos": "*",
    "sql-template-tag": "^5.2.1"
  }
}
