generator androidMarrocosClient {
  provider = "prisma-client-js"
  output   = "../../src/generated/prisma/android/marrocos"
}

datasource androidMarrocosDb {
  provider = "sqlserver"
  url      = env("ANDROID_MARROCOS")
}

model EnviosMarrocos {
  idEnvioMarrocos         Int                       @id(map: "PK_EnviosMarrocos") @default(autoincrement())
  nomeEnvio               String                    @androidMarrocosDb.NVarChar(50)
  dataTime                DateTime                  @androidMarrocosDb.DateTime
  estadoEnvio             Boolean
  estadoEmvioImp          Boolean
  estadoConteudo          Boolean
  estadoTerminado         Boolean
  nomeUser                String                    @androidMarrocosDb.VarChar(25)
  dataFim                 DateTime?                 @androidMarrocosDb.DateTime
  Users                   Users                     @relation(fields: [nomeUser], references: [nomeUser], onUpdate: NoAction, map: "FK_EnviosMarrocos_Users")
  EnviosMarrocosDestino   EnviosMarrocosDestino?
  EnviosMarrocosEnvioPeso EnviosMarrocosEnvioPeso[]
  EnviosMarrocosPaletes   EnviosMarrocosPaletes[]
  EnviosMarrocosPedidos   EnviosMarrocosPedidos[]
  EnviosMarrocosPrecos    EnviosMarrocosPrecos[]
}

